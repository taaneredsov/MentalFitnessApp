{
  "name": "mental-fitness-app",
  "private": true,
  "version": "0.1.6",
  "description": "Corporate Mental Fitness PWA with Airtable backend",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prebuild": "node scripts/bump-version.js",
    "build": "tsc -b && vite build",
    "build:server": "tsc --project tsconfig.server.json",
    "db:migrate": "node tasks/db-migrate.mjs",
    "worker:start": "npm run build:server && node dist-server/api/workers/sync-worker.js",
    "backfill:airtable-to-postgres": "npm run build:server && node dist-server/api/workers/backfill.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "validate:backfill": "node tasks/validate-backfill.mjs"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-query": "^5.90.16",
    "@tanstack/react-query-devtools": "^5.91.2",
    "airtable": "^0.12.2",
    "bcryptjs": "^3.0.3",
    "canvas-confetti": "^1.9.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cookie": "^1.1.1",
    "cookie-parser": "^1.4.7",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "i18next": "^25.8.6",
    "i18next-browser-languagedetector": "^8.2.1",
    "ioredis": "^5.9.1",
    "jose": "^6.1.3",
    "lucide-react": "^0.562.0",
    "nodemailer": "^7.0.12",
    "openai": "^6.16.0",
    "pg": "^8.18.0",
    "pulltorefreshjs": "^0.1.22",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.70.0",
    "react-i18next": "^16.5.4",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.12.0",
    "tailwind-merge": "^3.4.0",
    "web-push": "^3.6.7",
    "zod": "^3.25.76"
  },
  "optionalDependencies": {
    "@esbuild/darwin-arm64": "^0.27.3",
    "@rollup/rollup-darwin-arm64": "^4.57.1",
    "@tailwindcss/oxide-darwin-arm64": "^4.1.18",
    "lightningcss-darwin-arm64": "^1.31.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/canvas-confetti": "^1.9.0",
    "@types/cookie": "^0.6.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.6",
    "@types/node": "^24.10.1",
    "@types/nodemailer": "^7.0.9",
    "@types/pg": "^8.16.0",
    "@types/pulltorefreshjs": "^0.1.7",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/ui": "^4.0.17",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "jsdom": "^27.4.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^4.0.17"
  }
}
